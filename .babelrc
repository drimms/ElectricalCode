{
    "compact": false,
    "presets": [
      [
        "@babel/preset-env",
        {
          "loose": true,
          "targets": {
            "browsers": [
              "ie >= 11",
              "safari > 10"
            ]
          }
        }
      ],
      [
        "@babel/preset-typescript",
        {
          "isTSX": true,
          "allExtensions": true
        }
      ]
    ],
    "plugins": [
      [
        "babel-plugin-inferno",
        {
          "imports": true
        }
      ]
    ]
  }